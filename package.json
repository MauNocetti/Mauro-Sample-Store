{
  "name": "Uprising",
  "description": "A Shopify Theme Boilerplate Utilizing Preact, Swiper, Jquery, Tailwind CSS, and Liquid",
  "author": "Mauro Nocetti",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "run-p -sr shopify:serve webpack:watch",
    "deploy": "run-s webpack:build && cd shopify && shopify theme push",
    "deploy:new": "run-s webpack:build && cd shopify && shopify theme push --unpublished",
    "webpack:watch": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.dev.js --watch --progress",
    "webpack:build": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.prod.js --progress",
    "shopify:serve": "cd shopify && shopify theme serve",
    "shopify:pull": "cd shopify && shopify theme pull",
    "lint": "run-s -c lint:*",
    "lint:js": "eslint src/scripts/**/*.js --config .config/.eslintrc.js",
    "lint:css": "stylelint src/styles/**/*.{css,sass,scss} --config .config/.stylelintrc.js",
    "lint:shopify": "cd shopify && shopify theme check",
    "fix": "run-s -c fix:*",
    "fix:js": "eslint src/scripts/**/*.js --config .config/.eslintrc.js --fix",
    "fix:css": "stylelint src/styles/**/*.{css,sass,scss} --config .config/.stylelintrc.js --fix",
    "fix:shopify": "cd shopify && shopify theme check -a"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.5.0",
    "@vimeo/player": "^2.16.1",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "preact": "^10.5.14",
    "swiper": "^6.8.0",
    "tailwindcss": "^3.0.9",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.15.0",
    "eslint-webpack-plugin": "^3.0.1",
    "expose-loader": "^3.0.0",
    "glob": "^7.1.7",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.37.2",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-scss": "^3.20.1",
    "stylelint-webpack-plugin": "^3.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.48.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "webpack-remove-empty-scripts": "^0.7.1"
  }
}
